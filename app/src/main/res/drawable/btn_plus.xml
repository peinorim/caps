<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/brown_darken3" />
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="1000dp" android:topLeftRadius="0dp" android:topRightRadius="1000dp" />
            <padding android:bottom="5sp" android:left="21sp" android:right="21sp" android:top="5sp" />
